;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Nothing Calculator"
config/name_localized={
"es": "Calculadora nada",
"ru": "Калькулятор ничего",
"uk": "Калькулятор нічого"
}
config/version="1.1.0"
run/main_scene="uid://blohkbwwwpgap"
config/features=PackedStringArray("4.4", "GL Compatibility")
run/low_processor_mode=true
config/icon="res://assets/icon.svg"
config/windows_native_icon="res://assets/icon.ico"

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1
window/ios/allow_high_refresh_rate=false

[dotnet]

project/assembly_name="NothingCalculator"

[internationalization]

locale/translations=PackedStringArray("res://localization/translations.en.translation", "res://localization/translations.ru.translation", "res://localization/translations.uk.translation", "res://localization/translations.es.translation")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
